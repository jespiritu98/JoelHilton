@{
    ViewData["Title"] = "Deletion Confirmation";
}
@model MovieResponse


<h2>Confirm Deletion</h2>
<h3>Are you Sure you want to delete the record for ""@Model.Title"?</h3>

<form method="post" asp-action="Delete">

    <button type="submit">Delete</button>
    <a asp-controller="Home" asp-action="MovieList" class="btn btn-primary">Cancel</a>

    <input type="hidden" asp-for="ApplicationId"/>
</form>
<link href="~/css/site.css" rel="stylesheet" />